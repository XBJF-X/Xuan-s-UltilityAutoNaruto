    border: none;
    width: 0;  /* 隐藏默认按钮 */
    height: 0;
}



/* 悬停和选中状态 */
QSpinBox:hover {
    border-color: #39C5BB;
}

QSpinBox:focus {
    border-color: #39C5BB;
    outline: none;
}

/* 解决QSpinBox编辑区域与整体样式冲突 */
